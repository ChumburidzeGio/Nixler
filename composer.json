{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "ext-gd": "*",
        "artesaos/seotools": "^0.10.0",
        "barryvdh/laravel-debugbar": "^2.3",
        "cviebrock/eloquent-sluggable": "^4.2",
        "damiantw/laravel-scout-mysql-driver": "^1.0",
        "dimsav/laravel-translatable": "^7.0",
        "doctrine/dbal": "^2.5",
        "geoip2/geoip2": "~2.0",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^2.0",
        "laravel/socialite": "^2.0",
        "laravel/tinker": "~1.0",
        "menarasolutions/geographer": "^0.3.3",
        "mobiledetect/mobiledetectlib": "^2.8",
        "nwidart/laravel-modules": "^1.17",
        "overtrue/laravel-follow": "^1.0",
        "overtrue/socialite": "~1.0",
        "petercoles/multilingual-language-list": "^1.1",
        "plank/laravel-mediable": "^2.4",
        "plank/laravel-metable": "^1.0",
        "pusher/pusher-php-server": "^2.6",
        "rap2hpoutre/laravel-log-viewer": "^0.8.0",
        "silber/bouncer": "v1.0.0-beta.2",
        "spatie/laravel-newsletter": "^3.4",
        "torann/geoip": "^1.0",
        "watson/validating": "^3.0",
        "nixler/user": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "php artisan address:install",
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
