{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "ext-gd": "*",
        "artesaos/seotools": "^0.10.0",
        "ayeo/price": "^1.0",
        "barryvdh/laravel-debugbar": "^2.3",
        "bugsnag/bugsnag-laravel": "^2.0",
        "cviebrock/eloquent-sluggable": "^4.2",
        "cviebrock/eloquent-taggable": "^2.1",
        "damiantw/laravel-scout-mysql-driver": "^1.0",
        "dawson/amazon-ecs": "^2.0",
        "dimsav/laravel-translatable": "^7.0",
        "doctrine/dbal": "^2.5",
        "geoip2/geoip2": "~2.0",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3",
        "jenssegers/agent": "^2.5",
        "keen-io/keen-io": "^2.5",
        "laravel/dusk": "^1.0",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^2.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "league/fractal": "^0.15.0",
        "menarasolutions/geographer": "^0.3.3",
        "nlp-tools/nlp-tools": "^0.1.3",
        "nwidart/laravel-modules": "^1.17",
        "overtrue/laravel-follow": "^1.0",
        "petercoles/multilingual-language-list": "^1.1",
        "plank/laravel-mediable": "^2.4",
        "plank/laravel-metable": "^1.0",
        "prettus/l5-repository": "^2.6",
        "pusher/pusher-php-server": "^2.6",
        "rap2hpoutre/laravel-log-viewer": "^0.8.0",
        "recombee/php-api-client": "^1.3",
        "silber/bouncer": "v1.0.0-beta.2",
        "spatie/laravel-cookie-consent": "^1.8",
        "spatie/laravel-newsletter": "^3.4",
        "torann/geoip": "^1.0",
        "watson/validating": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan install"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
